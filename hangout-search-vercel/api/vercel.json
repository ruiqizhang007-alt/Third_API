{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/geo",
      "dest": "/api/geo.js"
    },
    {
      "src": "/api/places",
      "dest": "/api/places.js"
    },
    {
      "src": "/api/weather",
      "dest": "/api/weather.js"
    },
    {
      "src": "/api/route",
      "dest": "/api/route.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}